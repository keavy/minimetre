<% unless collection.blank? -%>
  <h2>Projects</h2>
    <table id="summary">
      <thead>
        <tr>
          <th></th>
          <th>Title</th>
          <th>URL</th>
        </tr>
      </thead>
      <tbody>
        <% collection.each do |project| -%>
          <tr>
            <td><%= link_to "Edit", edit_admin_project_path(project), :class => 'secondary' %></td>
            <td><%= project.title %></td>
            <td><%= project.url %></td>
          </tr>
        <% end -%>
      </tbody>
    </table>
<% end -%>